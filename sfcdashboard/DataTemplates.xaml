<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:sfcdashboard"
                    xmlns:vm="clr-namespace:sfcdashboard.ViewModel"
                    xmlns:view="clr-namespace:sfcdashboard.View">
    
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FileViewModel}">
        <view:FileView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ConnectionsViewModel}">
        <view:ConnectionsView/>
    </DataTemplate>


</ResourceDictionary>